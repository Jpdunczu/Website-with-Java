<form action="Authenticator" method="post">
    <div class="container-fluid">
        <div class="col-sm-2"></div>
            <div class="col-sm-8">
                <div class="panel panel-default">
                    <div class="panel-heading text-center">
                        <h2>Admin Console Login</h2>
                    </div>
                    <div class="panel-body">
                    <p><strong>username:</strong>
                        <input type="text" class="form-control" name="username"></p>
                    <p><strong>password:</strong>
                        <input type="password" class="form-control" name="password"></p>
                    </div>
                    <div class="panel-footer">
                        <button type="submit" name="action" value="login" class="btn btn-success btn-block"><span class="glyphicon glyphicon-off"></span> Login</button>
                    </div>
                </div>
            </div>
        <div class="col-sm-2"></div>
    </div>
</form>
